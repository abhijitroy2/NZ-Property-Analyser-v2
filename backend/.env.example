# Database
DATABASE_URL=sqlite:///./nz_property_finder.db

# TradeMe Scraper
TRADEME_SEARCH_URLS=https://www.trademe.co.nz/a/property/residential/sale/waikato/search?price_min=250000&price_max=400000

# Filters
MAX_PRICE=500000
MIN_POPULATION=50000

# AI Vision (for image analysis - optional)
# Set VISION_PROVIDER to "openai", "anthropic", "google", "vertex", or "mock"
VISION_PROVIDER=mock
OPENAI_API_KEY=
OPENAI_VISION_MODEL=gpt-4o-mini
OPENAI_SUMMARY_MODEL=gpt-4o-mini
ANTHROPIC_API_KEY=
# Google Cloud Vision - path to your service account JSON key file
GOOGLE_VISION_CREDENTIALS=credentials/google-vision-key.json
# Vertex AI Gemini - uses $500 free credit (VISION_PROVIDER=vertex)
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GOOGLE_CLOUD_LOCATION=us-central1

# Google Maps (for geocoding - optional)
GOOGLE_MAPS_API_KEY=

# Email Notifications (optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=
SMTP_PASSWORD=
EMAIL_FROM=
EMAIL_TO=

# Insurance API (optional)
INSURANCE_PROVIDER=mock

# Subdivision (use council zone APIs and rules when in scope)
SUBDIVISION_USE_COUNCIL_RULES=true

# Scheduler
ENABLE_SCHEDULER=false
SCHEDULER_HOUR=7
SCHEDULER_MINUTE=0

# CORS
FRONTEND_URL=http://localhost:3000
